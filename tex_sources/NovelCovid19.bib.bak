  eprint        = {2003.02985},
  file          = {:home/saul/Documents/Mendeley Desktop/Jia et al/Electronic Journal of Differential Equations/Jia et al. - 2020 - Modeling the control of COVID-19 Impact of policy interventions and meteorological factors.pdf:pdf},
  keywords      = {COVID-19,Dynamical model,Isolation strategy,Meteorological index,Vaccination strategy},
}

