@Article{Huang2010,
  author   = {Huang, Gang and Takeuchi, Yasuhiro and Ma, Wanbiao and Wei, Daijun},
  title    = {{Global Stability for Delay SIR and SEIR Epidemic Models with Nonlinear Incidence Rate}},
  journal  = {Bulletin of Mathematical Biology},
  year     = {2010},
  volume   = {72},
  number   = {5},
  pages    = {1192--1207},
  issn     = {00928240},
  abstract = {In this paper, based on SIR and SEIR epidemic models with a general nonlinear incidence rate, we incorporate time delays into the ordinary differential equation models. In particular, we consider two delay differential equation models in which delays are caused (i) by the latency of the infection in a vector, and (ii) by the latent period in an infected host. By constructing suitable Lyapunov functionals and using the Lyapunov-LaSalle invariance principle, we prove the global stability of the endemic equilibrium and the disease-free equilibrium for time delays of any length in each model. Our results show that the global properties of equilibria also only depend on the basic reproductive number and that the latent period in a vector does not affect the stability, but the latent period in an infected host plays a positive role to control disease development. {\textcopyright} 2009 Society for Mathematical Biology.},
  doi      = {10.1007/s11538-009-9487-6},
  file     = {:home/saul/Documents/Mendeley Desktop/Huang et al/Bulletin of Mathematical Biology/Huang et al. - 2010 - Global Stability for Delay SIR and SEIR Epidemic Models with Nonlinear Incidence Rate.pdf:pdf},
  keywords = {Global stability,Lyapunov functional,Nonlinear incidence rate,Time delay},
}

